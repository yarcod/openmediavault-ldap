msgstr "PAM inschakelen"

msgid "Enable SSL/TLS"
msgstr "SSL/TLS inschakelen"

msgid "Enable secure connection."
msgstr "Beveiligde verbinding inschakelen"

msgid "Extra client options"
msgstr "Extra client instellingen"

msgid "Extra options"
msgstr "Extra opties"

msgid "General settings"
msgstr "Algemene instellingen"

msgid "Groups suffix"
msgstr "Groep suffix"

