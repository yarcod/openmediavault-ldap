msgstr "Ativar"

msgid "Enable PAM"
msgstr "Ativar PAM"

msgid "Enable SSL/TLS"
msgstr "Ativar SSl/TLS"

msgid "Enable secure connection."
msgstr "Ativar ligação segura."

msgid "Extra client options"
msgstr "Opções extra de cliente"

msgid "Extra options"
msgstr "Opções extras"

msgid "General settings"
msgstr "Definições gerais"

msgid "Groups suffix"
msgstr "Sufixo de grupos"

